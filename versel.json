{
    "version": 2,
    "builds": [
      {
        "src": "api/projects.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/projects",
        "dest": "api/projects.js"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "includeFiles": ["api/**"]
      }
    }
  }